<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

    <skin>
        <groupId>com.citytechinc.maven</groupId>
        <artifactId>citytech-maven-skin</artifactId>
        <version>1.0.0</version>
    </skin>

    <bannerRight>
        <name>${project.name}</name>
        <href>http://code.citytechinc.com/cq-groovy-console</href>
    </bannerRight>

    <googleAnalyticsAccountId>UA-928005-8</googleAnalyticsAccountId>

    <custom>
        <reflowSkin>
            <skinAttribution>false</skinAttribution>
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>true</smoothScroll>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <breadcrumbs>false</breadcrumbs>
            <markPageHeader>false</markPageHeader>
            <toc>false</toc>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <topNav>Overview|reports</topNav>
            <brand>
                <name>${project.name}</name>
                <href>http://code.citytechinc.com/cq-groovy-console</href>
            </brand>
            <slogan position="bannerRight"></slogan>
            <body />
            <bottomNav>
                <column>Overview</column>
                <column>reports</column>
            </bottomNav>
        </reflowSkin>
    </custom>

    <body>
        <links>
            <item name="GitHub" href="https://github.com/Citytechinc/cq-groovy-console" />
        </links>
        <menu name="Overview">
            <item name="Introduction" href="index.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
</project>